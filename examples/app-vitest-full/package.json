{
  "private": true,
  "name": "example-app-vitest-full",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:prepare": "nuxt prepare",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "test:dev": "nuxt dev --no-fork",
    "test:unit": "vitest",
    "test:types": "nuxt prepare && vue-tsc --noEmit",
    "test:jsdom": "VITEST_DOM_ENV=jsdom pnpm test:unit --run",
    "test:happy-dom": "VITEST_DOM_ENV=happy-dom pnpm test:unit --run",
    "test": "pnpm test:happy-dom && pnpm test:jsdom && pnpm test:dev && pnpm test:types"
  },
  "devDependencies": {
    "@nuxt/test-utils": "latest",
    "@testing-library/vue": "8.1.0",
    "@vitest/ui": "4.0.17",
    "happy-dom": "20.1.0",
    "jsdom": "27.4.0",
    "listhen": "1.9.0",
    "nuxt": "4.2.2",
    "typescript": "5.9.3",
    "vitest": "4.0.17",
    "vue-tsc": "3.2.2"
  },
  "dependencies": {
    "idb-keyval": "^6.2.2"
  }
}
