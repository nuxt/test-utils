{
  "default": {
    "paths": [
      "test/acceptance/features/**/*.feature"
    ],
    "import": [
      "test/acceptance/**/*.ts"
    ],
    "publish": false,
    "format": [
      "summary",
      "progress-bar",
      "html:tests/acceptance/reports/cucumber-report.html",
      "junit:tests/acceptance/reports/junit.xml",
      "json:tests/acceptance/reports/report.json",
      "message:tests/acceptance/reports/message.json"
    ]
  }
}
