    },
    "playwright-core": {
      "optional": true
    },
    "@vitest/ui": {
      "optional": true
    },
    "vitest": {
      "optional": true
    }
  },
  "resolutions": {
    "@nuxt/test-utils": "workspace:*",
    "rollup": "4.7.0",
    "vite": "5.0.7"
  },
  "engines": {
    "node": "^14.18.0 || >=16.10.0"
  },
  "packageManager": "pnpm@8.12.0"
}
