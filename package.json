{
  "name": "@nuxt/test-utils",
  "version": "0.2.2",
  "description": "Test utilities for Nuxt.js",
  "repository": "nuxt-community/module-test-utils",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "jest-preset.js",
    "environment.js"
  ],
  "scripts": {
    "build": "siroc build",
    "lint": "eslint --ext .ts,.js,.vue .",
    "release": "yarn test && standard-version && yarn build && git push --follow-tags && npm publish",
    "test": "yarn lint && yarn build && yarn test:unit && yarn test:e2e",
    "test:e2e": "yarn build && jest test/e2e --forceExit",
    "test:unit": "yarn build && jest test/unit --forceExit"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "consola": "^2.15.3",
    "defu": "^5.0.0",
    "listhen": "^0.2.4",
    "ohmyfetch": "^0.2.0",
    "ufo": "^0.7.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "latest",
    "@babel/preset-env": "latest",
    "@nuxt/types": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/jest": "latest",
    "@types/node": "latest",
    "babel-jest": "latest",
    "codecov": "latest",
    "core-js": "latest",
    "eslint": "latest",
    "jest": "latest",
    "nuxt-edge": "latest",
    "playwright": "latest",
    "siroc": "latest",
    "standard-version": "latest",
    "typescript": "latest"
  }
}
