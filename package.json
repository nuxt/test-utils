    "magic-string": "^0.30.5",
    "node-fetch-native": "^1.4.1",
    "ofetch": "^1.3.3",
    "pathe": "^1.1.1",
    "perfect-debounce": "^1.0.0",
    "radix3": "^1.1.0",
    "std-env": "^3.6.0",
    "ufo": "^1.3.2",
    "unenv": "^1.8.0",
    "unplugin": "^1.5.1",
    "vitest-environment-nuxt": "1.0.0-alpha.0"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
