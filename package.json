    "@testing-library/vue": "8.0.1",
    "@types/estree": "1.0.5",
    "@types/jsdom": "21.1.6",
    "@types/semver": "7.5.6",
    "@vitest/ui": "1.0.4",
    "@vue/test-utils": "2.4.3",
    "changelogen": "0.5.5",
    "eslint": "8.55.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsdoc": "46.9.0",
    "eslint-plugin-no-only-tests": "3.1.0",
    "eslint-plugin-unicorn": "49.0.0",
    "h3": "1.9.0",
    "jiti": "1.21.0",
    "nuxt": "3.8.2",
    "playwright-core": "1.40.1",
    "rollup": "4.7.0",
    "semver": "7.5.4",
    "unbuild": "latest",
