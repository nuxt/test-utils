{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    ".": {
      "entry": [
        "src/config.ts",
        "src/e2e.ts",
        "src/playwright.ts",
        "src/runtime/**/*.{js,ts,mjs,vue}",
        "src/experimental.ts",
        "src/vitest-environment.ts",
        "test/fixtures/**/*.ts"
      ],
      "ignoreFiles": [
        "test/unit/__snapshots__/**/*"
      ],
      "ignoreUnresolved": [
        "#build/root-component.mjs",
        "#app/nuxt-vitest-app-entry"
      ],
      "ignoreDependencies": [
        "nuxt-vitest-environment-options",
        "vitest-environment-nuxt",
        "@cucumber/cucumber",
        "@jest/globals",
        "@playwright/test",
        "@testing-library/vue",
        "@vue/test-utils",
        "happy-dom",
        "jsdom",
        "playwright-core",
        "vitest"
      ]
    }
  },
  "ignoreWorkspaces": [
    "examples/**"
  ]
}
